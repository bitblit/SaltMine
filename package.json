  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.3.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.31",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "@types/node": "6.0.46",
    "aws-sdk": "2.503.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "mocha": "6.2.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "2.7.1"
  }
