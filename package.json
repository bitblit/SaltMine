{
  "name": "@bitblit/saltmine",
  "version": "0.4.4",
  "description": "A system for automating processing of work on lambda using a combo of Lambda/SQS/SNS efficiently",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "author": "Christopher Weiss <bitblit@gmail.com>",
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc",
    "watch": "tsc -w .",
    "clean": "rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/SaltMine"
  },
  "keywords": [
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/SaltMine/issues"
  },
  "homepage": "https://github.com/bitblit/SaltMine#readme",
  "engines": {
    "node": ">=12.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.8.8"
  },
  "resolutions": {
    "lodash": "4.17.19",
    "minimist": "1.2.5",
    "acorn": "7.4.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.60",
    "@types/chai": "4.2.12",
    "@types/mocha": "8.0.1",
    "@types/node": "12.12.6",
    "aws-sdk": "2.730.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "eslint": "7.6.0",
    "husky": "4.2.5",
    "mocha": "8.1.1",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  }
}
