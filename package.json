{
  "name": "@bitblit/saltmine",
  "version": "0.4.3",
  "description": "A system for automating processing of work on lambda using a combo of Lambda/SQS/SNS efficiently",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "author": "Christopher Weiss <bitblit@gmail.com>",
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc",
    "watch": "tsc -w .",
    "clean": "rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/SaltMine"
  },
  "keywords": [
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/SaltMine/issues"
  },
  "homepage": "https://github.com/bitblit/SaltMine#readme",
  "engines": {
    "node": ">=12.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.5.14"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.40",
    "@types/chai": "4.2.9",
    "@types/mocha": "7.0.1",
    "@types/node": "12.12.6",
    "aws-sdk": "2.618.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "eslint": "6.8.0",
    "husky": "3.0.5",
    "mocha": "7.0.1",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
